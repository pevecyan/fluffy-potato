        mc:Ignorable="d"
        Title="REGEX" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" WindowStyle="ToolWindow" VerticalAlignment="Top" Height="80" BorderBrush="#FFB64A4A" Topmost="True">
